<?xml version = "1.0" encoding = "UTF-8"?>

<beans xmlns = "http://www.springframework.org/schema/beans"
   xmlns:xsi = "http://www.w3.org/2001/XMLSchema-instance"
   xsi:schemaLocation = "http://www.springframework.org/schema/beans
   http://www.springframework.org/schema/beans/spring-beans.xsd">
   
   <bean id = "addr" class = "edureka.Address">
   	  <property name="city" value="somecity"></property>
   	  <property name="state" value="somestate"></property>
   	  <property name="zipCode" value="500001"></property>
   </bean>
   
   <bean id = "emp" class = "edureka.Employee">
   	  <property name="eid" value="101"></property>
   	  <property name="ename" value="Goodman"></property>
   	  <property name="eaddress" ref="addr"></property>
   </bean>

</beans>